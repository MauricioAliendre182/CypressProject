{
  "name": "cypressproject",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cucumber:tags:report": "cypress run --env tags=@Acceptance updateSnapshots=true allure=true --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "mochawesome:report": "npx mochawesome-merge \"cypress/results/mochawesome/*.json\"> mochawesome.json && npx marge mochawesome.json",
    "junit:report": "jrm cypress/results/junit/combined-report.xml \"cypress/results/junit/*.xml\"",
    "delete:results": "rm -r cypress/results/* || true",
    "allure:serve": "allure serve allure-results",
    "allures:serve:docker": "allure serve dockerReports",
    "run:dashboard": "cypress run --record -key ****",
    "run:dashboard:parallel": "cypress run --record -key **** --parallel"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.17.1",
    "cypress": "^12.14.0",
    "cypress-real-events": "^1.10.0",
    "cypress-xpath": "^2.0.1",
    "prettier": "^3.0.1"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.4",
    "@shelex/cypress-allure-plugin": "^2.40.0",
    "cypress-image-snapshot": "^4.0.1",
    "cypress-multi-reporters": "^1.6.3",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "webpack": "^5.88.2"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/e2e/features/**/*.{js,ts}",
      "cypress/e2e/[filepath].{js,ts}",
      "cypress/support/step_definitions/**/*.{js,ts}"
    ]
  }
}
